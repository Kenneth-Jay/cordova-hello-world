<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.kjay.helloworld" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>helloworld</name>
<js-module name="helloworld" src="www/helloworld.js">
<clobbers target="cordova.plugins.helloworld" />
</js-module><platform name="android">
<config-file parent="/*" target="res/xml/config.xml">
<feature name="helloworld"><param name="android-package" value="com.kjay.helloworld.helloworld" />
</feature></config-file><config-file parent="/*" target="AndroidManifest.xml" />
<source-file src="src/android/helloworld.java" target-dir="src/com/kjay/helloworld/helloworld" />
</platform><platform name="ios"><config-file parent="/*" target="config.xml">
<feature name="helloworld"><param name="ios-package" value="helloworld" /></feature>
</config-file><source-file src="src/ios/helloworld.m" /></platform>
</plugin>